<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jasmine.dao.UsersMapper">
	<resultMap id="userResultMap" type="com.jasmine.pojo.Users">
		<id column="id" property="id" javaType="integer"/>
		<result column="name" property="name" javaType="string"/>
		<result column="age" property="age" javaType="integer"/>
	</resultMap>
	<select id="getAllUsers" resultType="com.jasmine.pojo.Users">
		select * from users
	</select>
</mapper>
